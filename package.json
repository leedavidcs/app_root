{
  "name": "app_root",
  "version": "0.0.1",
  "description": "Application root, for development purposes.",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "dotenv -- next -p 3031",
    "build": "dotenv -- next build",
    "start": "next start",
    "storybook": "dotenv -e .env.storybook -- start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "audit-ci": "audit-ci --moderate --report-type full --package-manager npm",
    "build-docker": "echo $(date +%s) && docker-compose build --build-arg CACHE_BUST_FILES_UPDATED=$(date +%s) --pull server",
    "generate:backend": "npm-run-all generate:prisma generate:nexus generate:emails generate:options",
    "generate:emails": "npm run with-script -- src/server/emails/generate",
    "generate:icons": "npm run with-script -- scripts/generate-icons-module.script",
    "generate:nexus": "npm run with-script -- src/server/graphql/nexus/schema --nexus-exit",
    "generate:prisma": "prisma generate",
    "generate:options": "npm run with-script -- scripts/generate-data-key-options.script",
    "generate:frontend": "graphql-codegen",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run generate:backend",
    "deps:start": "docker-compose up server-dependencies",
    "deps:stop": "docker-compose down",
    "test": "echo \"Error: no test specified\" && exit 1",
    "with-script": "dotenv -- ts-node -P tsconfig.gen.json -r tsconfig-paths/register -T"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mua-inc/app_root.git"
  },
  "author": "leedavidcs",
  "license": "ISC",

{
	"presets": [
		[
			"next/babel",
			{
				"preset-env": {
					"corejs": "3",
					"modules": false,
					"useBuiltIns": "usage"
				}
			}
		]
	],
	"plugins": [
		[
			"module-resolver",
			{
				"alias": {
					"@/client": "./src/client",
					"@/server": "./src/server"
				}
			}
		],
		"date-fns",
		"lodash"
	]
}
